cmd - j : yabai -m window --focus next
cmd - k : yabai -m window --focus prev
cmd + shift - k : yabai -m display --focus next
cmd + shift - j : yabai -m display --focus prev
cmd + shift - h : yabai -m space --focus prev
cmd + shift - l : yabai -m space --focus next
shift + cmd - 1 : yabai -m window --space  1; yabai -m space --focus 1
shift + cmd - 2 : yabai -m window --space  2; yabai -m space --focus 2
shift + cmd - 3 : yabai -m window --space  3; yabai -m space --focus 3
shift + cmd - 4 : yabai -m window --space  4; yabai -m space --focus 4
shift + cmd - 5 : yabai -m window --space  5; yabai -m space --focus 5
shift + cmd - 6 : yabai -m window --space  6; yabai -m space --focus 6
shift + cmd - 7 : yabai -m window --space  7; yabai -m space --focus 7
shift + cmd - 8 : yabai -m window --space  8; yabai -m space --focus 8
shift + cmd - 9 : yabai -m window --space  9; yabai -m space --focus 9
shift + cmd - 0 : yabai -m window --space  0; yabai -m space --focus 0
shift + ctrl - left : yabai -m window --move rel:-20:0
shift + ctrl - right : yabai -m window --move rel:20:0
shift + ctrl - down : yabai -m window --move rel:0:20
shift + ctrl - up : yabai -m window --move rel:0:-20
shift + alt - left : yabai -m window --resize left:-20:0
shift + alt - right : yabai -m window --resize left:20:0
shift + alt - up : yabai -m window --resize top:0:-20
shift + alt - down : yabai -m window --resize top:0:20
alt - d : yabai -m window --toggle zoom-parent
alt - f : yabai -m window --toggle zoom-fullscreen
cmd + ctrl - m : yabai -m config focus_follows_mouse autofocus
cmd + alt - m : yabai -m config focus_follows_mouse off
shift + cmd - z : yabai -m window --space prev; yabai -m space --focus prev
shift + cmd - c : yabai -m window --space next; yabai -m space --focus next
shift + cmd - x : yabai -m window --display prev; yabai -m display --focus prev
shift + cmd - v : yabai -m window --display next; yabai -m display --focus next
alt - return : /Applications/kitty.app/Contents/MacOS/kitty --single-instance -d ~
cmd + ctrl - b : open -a Bitwarden
cmd + ctrl - f : open -na "Finder"
cmd + ctrl - s : open -a "Safari"
cmd + ctrl - j : scjpg; sleep 3
cmd + alt - s : yabai -m window --toggle pip; yabai -m window --toggle sticky
cmd - space : yabai -m window --toggle float;
cmd + alt - g : yabai -m window --toggle float; \
	yabai -m window --toggle sticky; \
	yabai -m window --grid 4:4:1:1:2:2
